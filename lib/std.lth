type int = simple int;

addition : ^int, int -> int;
addition := ^(x : int, y : int) -> x @add y;

subtraction : ^int, int -> int;
subtraction := ^(x : int, y : int) -> x @sub y;

multiplication : ^int, int -> int;
multiplication := ^(x : int, y : int) -> x @mul y;

division : ^int, int -> int;
division := ^(x : int, y : int) -> x @div y;
